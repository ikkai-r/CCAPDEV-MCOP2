<!-- Content Body -->
      <div class="container mt-4 pt-3">
        <div class="container d-flex content-container mt-4">

            <!-- Search Results Container -->
            <div class="container flex-grow-1">
                <nav>
                    <div class="nav nav-tabs d-flex justify-content-center search-results-navbar" id="nav-search-tab" role="tablist">
                        <button class="nav-link active profile-btn" id="nav-search-posts-tab" data-bs-toggle="tab" data-bs-target="#nav-search-posts" type="button" role="tab" aria-controls="nav-posts" aria-selected="true">
                            posts
                            <div class="profile-line"></div>
                        </button>                          
                          <button class="nav-link profile-btn" id="nav-search-tags-tab" data-bs-toggle="tab" data-bs-target="#nav-search-tags" type="button" role="tab" aria-controls="nav-comments" aria-selected="false">
                            tags
                            <div class="profile-line"></div>
                        </button>    
                        <button class="nav-link profile-btn" id="nav-search-accounts-tab" data-bs-toggle="tab" data-bs-target="#nav-search-accounts" type="button" role="tab" aria-controls="nav-comments" aria-selected="false">
                            accounts
                            <div class="profile-line"></div>
                        </button>                 
                    </div>
                </nav>

            <div class="tab-content search-tab-content" id="nav-search-tab-content">
                <div class="tab-pane fade show active" id="nav-search-posts" role="tabpanel" aria-labelledby="nav-search-posts-tab-pane">
                
                        {{#if searched_post.length}}
                            {{#each searched_post}}
                            {{> post_preview}}
                            {{/each}}
                        {{else}}
                            
                                <div class="text-center">
                                        <h5><b>It's empty...</b></h5>
                                        <p>Try searching for something else.</p>
                                </div>
                        
                        {{/if}}
    
                    </div>
               

                <div class="tab-pane fade" id="nav-search-tags" role="tabpanel"aria-labelledby="nav-search-tags-tab-pane">
                    <!-- Tags Container -->
                    <div class="container">
                        <div class="row" style="display: flex; justify-content: center;"></div>
                    {{#each searched_tags}}
                            {{#each_upto 3, }}
                        
                            <div class="tag-group col-md-3" style="margin-right: 5vh;">
                                <div><h6 class="tag-group-name">#virus</h4></div>
                                <div class="tag-group-img" style="background-image:url(img/virus.png);"></div>
                                <div><h6 class="tag-group-count">523 posts</h4></div>
                            </div>
                            <div class="tag-group mr-5 col-md-3" style="margin-right: 5vh;">
                                <div><h6 class="tag-group-name">#help</h4></div>
                                <div class="tag-group-img" style="background-image:url(img/help.jpg);"></div>
                                <div><h6 class="tag-group-count">5k posts</h4></div>
                            </div>
                            <div class="tag-group mr-5 col-md-3">
                                <div><h6 class="tag-group-name">#techsupport</h4></div>
                                <div class="tag-group-img" style="background-image:url(img/techsupport.png);"></div>
                                <div><h6 class="tag-group-count">5.3k posts</h4></div>
                            </div>
                       
                    
                    {{/each}}
                        </div>
                    </div>
                </div>
                
                <div class="tab-pane fade" id="nav-search-accounts" role="tabpanel"aria-labelledby="nav-search-accounts-tab-pane">
                    <!-- Accounts Container -->
                    <div class="container">
                        <div class="row" style="display: flex; justify-content: center;">
                            <div class="search-account card col-md-3">    
                                <div class="card-head">
                                    <div class="account-card-img" style="background-image:url(img/redqueen.png);"></div>
                                    <a href="redqueen.html"><div class="account-card-username">RedQueen</div></a>
                                </div>
                                <div class="card-body">
                                    <div class="account-card-bio">The truth is what I make it. I could set this world on fire and call it rain.</div>
                                </div>
                            </div>
                            <div class="search-account card col-md-3">
                                <div class="card-head">
                                    <div class="account-card-img" style="background-image:url(img/sansfangirlxoxo.png);"></div>
                                    <a href="sansfangirlxoxo.html"><div class="account-card-username">sansfangirlxoxo</div></a>
                                </div>
                                <div class="card-body">
                                    <div class="account-card-bio">sans is my senpai desu :3</div>
                                </div>
                            </div>
                            <div class="search-account card col-md-3">
                                <div class="card-head">
                                    <div class="account-card-img" style="background-image:url(img/sherlockholmes.png);"></div>
                                    <a href="sherlockholmes.html"><div class="account-card-username">SherlockHolmes</div></a>
                                </div>
                                <div class="card-body">
                                    <div class="account-card-bio"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
 </div>
            </div>
                


            </div>
        
        </div>
